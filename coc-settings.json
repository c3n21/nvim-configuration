{
    "eslint.autoFix": false,
    "eslint.packageManager": "npm",
    "eslint.filetypes": [
        "javascript",
        "javascriptreact"
    ],
    "languageserver": {
        "intelephense": {
            "command": "intelephense",
            "args": ["--stdio"],
            "filetypes": ["php"],
            "initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"]
        }
    }
}
