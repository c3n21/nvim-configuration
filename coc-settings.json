{
    "languageserver": {
        "intelephense": {
            "command": "intelephense",
            "args": ["--stdio"],
            "filetypes": ["php"],
            "initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"]
        }
    },
    "suggest.timeout": 2000,
    "suggest.completionItemKindLabels": {
      "variable": "",
      "method": "", 
      "field": "", 
      "interface": "", 
      "property": " ", 
      "default": "", 
      "enum": "", 
      "function": "", 
      "constant": "", 
      "struct": "", 
      "module": "", 
      "typeParameter": "", 
      "class": ""
    },
    "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "typescript.tsx"]
}
